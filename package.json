{
  "name": "mapkitjs-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker that generates JWT for MapKit JS",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development yarn build",
    "format": "prettier --ignore-unknown --write '**/*'",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "yarn test:clean && yarn transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test"
  },
  "author": "Tse Kit Yam <me@kytse.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^8.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "itty-router": "^2.4.2",
    "jsonwebtoken": "^8.5.1"
  }
}
